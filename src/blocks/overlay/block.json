{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "utksds/overlay",
	"version": "0.1.0",
	"title": "Image with Text Overlay",
	"category": "design",
	"icon": "editor-table",
	"description": "",
	"supports": {
		"html": false
	},
	"attributes": {
		"imageAlt": {
			"type": "string",
			"default": ""
		},
		"imageUrl": {
			"type": "string",
			"default": ""
		},
		"overColor": {
			"type": "object",
			"default": { "name": "Dark", "slug": "bg-dark", "color": "#4b4b4b", "text": "text-white" }
		},
		"overOpacity": {
			"type": "integer",
			"default": 0
		}
	},
	"providesContext": {
		"overlay/bgColor": "overColor",
		"overlay/opacity": "overOpacity"
	},
	"textdomain": "utksds",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}