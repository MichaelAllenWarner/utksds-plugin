{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "utksds/alert",
	"version": "0.1.0",
	"title": "Alert",
	"category": "design",
	"icon": "smiley",
	"description": "",
	"supports": {
		"html": false
	},
	"attributes": {
		"imagePostion": {
			"type": "object",
			"default": { "name": "Smokey", "slug": "alert-smokey", "color": "#58595B", "text": "text-white"}
		},
		"text": {
			"type": "string",
			"source": "html",
			"selector": "span"
		},
		"placeholder": {
			"type": "string",
			"default": "Enter alert text here."
		}
	},
	"textdomain": "utksds",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}