{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "utksds/strip",
	"version": "0.1.0",
	"title": "Content Strip",
	"category": "utdesign_layout",
	"icon": "editor-table",
	"description": "",
	"supports": {
		"html": false
	},
	"attributes": {
		"imagePostion": {
			"type": "object",
			"default": { "name": "Smokey", "slug": "strip-smokey", "color": "#58595B", "text": "text-white" }
		},
		"spacing": {
			"type": "integer",
			"default": 0
		},
		"padding":{
			"type": "string",
			"default": "py-0"
		},
		"bgImage":{
			"type": "object"
		}
	},
	"textdomain": "utksds",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}