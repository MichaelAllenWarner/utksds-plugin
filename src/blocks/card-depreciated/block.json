{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "utksds/card",
	"version": "0.1.0",
	"title": "Depreciated Card",
	"category": "design",
	"icon": "list-view",
	"description": "A flexible and extensible content container for highlighting pieces of content.",
	"supports": {
		"html": false
	},
	"attributes": {
		"blockName": {
			"type": "string",
			"default": "utkwds/card"
		},
		"cardColor": {
			"type": "object",
			"default": { "name": "Light", "slug": "bg-light", "color": "#F6F6F6", "text": "text-dark" }
		},
		"cardOutline": {
			"type": "boolean",
			"default": false
		},
		"textColor": {
			"type": "string",
			"default": "text-dark"
		}
	},
	"providesContext": {
    	"card/blockName": "blockName",
		"card/cardColor": "cardColor",
		"card/cardOutline": "cardOutline"
	},
	"textdomain": "utkwds",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}